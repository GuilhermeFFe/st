$start
    pushr @a #10
    call $$square
    prnt @a
    call $$double
    prnt @a
    call $$divide
    prnt @a
    call $$subtract
    prnt @a
    push @a
    prnt
    hlt
$square
    pushr @b @a
    mult @a @b
    ret
$double
    pushr @b #2
    mult @a @b
    ret
$divide
    pushr @b #4
    div @a @b
    ret
$subtract
    pushr @b #5
    sub @a @b
    ret